{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "description": "Moved event from pubsub",
  "type": "object",
  "properties": {
    "payload": {
      "description": "Payload of moved event request",
      "type": "object",
      "properties": {
        "itemBarcode": {
          "description": "Item barcode from moved event",
          "type": "string"
        },
        "requests": {
          "description": "Original and updated requests from moved event",
          "type": "object",
          "properties": {
            "updated": {
              "description": "Updated request",
              "$ref": "eventRequest.json"
            }
          }
        }
      }
    }
  },
  "additionalProperties": false,
  "required": []
}
